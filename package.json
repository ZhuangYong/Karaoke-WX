  "scripts": {
    "dev": "npm run copy:dev && cross-env NODE_ENV=development webpack-dev-server --env=dev --inline --content-base ./build --config ./src/config/webpack.config.js --port 81 --history-api-fallback",
    "dev:host": "npm run copy:dev && cross-env NODE_ENV=development webpack-dev-server --env=dev --inline --content-base ./build --config ./src/config/webpack.config.js --port 81 --host wx.j-make.cn --history-api-fallback",
    "dev:expand": "npm run copy:dev && cross-env NODE_ENV=expand webpack-dev-server --env=expandTest --inline --content-base ./build --config ./src/config/webpack.config.js --port 81 --host wx.j-make.cn --history-api-fallback",
    "build:test": "npm run copy:dev && cross-env NODE_ENV=development webpack --config ./src/config/webpack.config.js --env=test",
    "build:expand": "npm run copy:dev && cross-env NODE_ENV=expand webpack --config ./src/config/webpack.config.js --env=expand",
    "build:master": "npm run copy:dev && cross-env NODE_ENV=master webpack --config ./src/config/webpack.config.js --env=master",
    "build:pre": "npm run copy:dev && cross-env NODE_ENV=pre webpack --config ./src/config/webpack.config.js --env=prod.pre",
    "正式发布编译": "npm run copy:dev && cross-env NODE_ENV=production webpack --config ./src/config/webpack.config.js --env=prod",
    "eslint": "eslint -c ./src/config/.eslintrc.js --ignore-path ./src/config/.eslintignore --ext .js ./src",
    "build & test": "npm run build && cross-env NODE_ENV=test karma start ./test/config/karma.conf.js --single-run",
    "test": "cross-env NODE_ENV=test karma start ./test/config/karma.conf.js --single-run",
    "clean": "babel-node tools/run clean",
    "copy:dev": "npm run clean && babel-node tools/run copydev"
